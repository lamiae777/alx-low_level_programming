#include "main.h"

/**
 * print_numbers - Prints numbers from 0 to 9 followed by a new line.
 */
void print_numbers(void)
{
    int i;

    for (i = 0; i < 10; i++)
    {
        _putchar(i + '0'); // Convert the number to its ASCII character representation
    }

    _putchar('\n'); // Output a new line after printing the numbers
}

